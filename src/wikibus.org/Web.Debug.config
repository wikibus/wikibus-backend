<?xml version="1.0"?>

<!-- For more information on using web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="wikibus#baseUrl" value="http://wikibus-test.gear.host/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="wikibus#apiUrl" value="http://wikibus-data-test.gear.host/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
    <add key="wikibus#websiteUrl" value="http://wikibus.herokuapps.com/" xdt:Transform="Replace" xdt:Locator="Match(key)" />
  </appSettings>

  <system.web>
    <customErrors mode="Off" xdt:Transform="Insert" />
  </system.web>
</configuration>