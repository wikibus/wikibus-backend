{
    "bibo": "http://purl.org/ontology/bibo/",
    "dcterms": "http://purl.org/dc/terms/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "langIso": "http://www.lexvo.org/page/iso639-1/",
    "opus": "http://lsdis.cs.uga.edu/projects/semdis/opus#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "sch": "http://schema.org/",
    "wbo": "http://wikibus.org/ontology#",
    "year": { "@id": "opus:year", "@type": "xsd:gYear" },
    "month": { "@id": "opus:month", "@type": "xsd:gMonth" },
    "date": { "@id": "dcterms:date", "@type": "xsd:date" },
    "pages": { "@id": "bibo:pages", "@type": "xsd:integer" },
    "title": "dcterms:title",
    "code": "dcterms:identifier",
    "description": "rdfs:comment",
    "languages": { "@id": "dcterms:language", "@container": "@set", "@type": "@id" },
    "isbn": "sch:isbn",
    "author": "sch:author",
    "name": "sch:name",
    "image": "sch:image",
    "contentUrl": { "@id": "sch:contentUrl", "@type": "sch:URL" },
    "thumbnail": "sch:thubmnail",
    "number": "sch:issueNumber",
    "magazine":  "sch:isPartOf"
}